# Environment variables for WebUntis Auto-Login
# Copy this file to .env and fill in your credentials

# FSV SSO Credentials
FSV_USERNAME=your_username_here
FSV_PASSWORD=your_password_here

# Optional: Discord Webhook for change notifications
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_url_here

# Optional: Browser settings
HEADLESS_MODE=false

